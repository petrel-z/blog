{
  "name": "@ort/vitepress-theme",
  "version": "0.1.0",
  "description": "A simple Vitepress theme",
  "homepage": "",
  "private": false,
  "keywords": [
    "vitepress",
    "blog"
  ],
  "exports": {
    "./node": {
      "types": "./node.d.ts",
      "import": "./node.mjs",
      "require": "./node.js",
      "default": "./node.js"
    },
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "node.d.ts",
    "node.js",
    "node.mjs",
    "docs",
    "src",
    "types"
  ],
  "bin":  {
    "create-vitepress-theme": "./cli.js"
  },
  "scripts": {
    "dev": "npm run build:node && npm run dev:docs",
    "dev:docs": "vitepress dev docs",
    "dev:node": "npx tsup --watch",
    "build": "npm run build:node && npm run build:docs",
    "build:docs": "vitepress build docs",
    "build:node": "npx tsup",
    "serve": "vitepress serve docs"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.7"
  },
  "dependencies": {
    "@giscus/vue": "^2.4.0",
    "@mdit-vue/shared": "^0.12.1",
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@vue/shared": "^3.4.26",
    "@vueuse/core": "^9.13.0",
    "markdown-it-task-checkbox": "^1.0.6",
    "mermaid": "^10.9.0",
    "swiper": "^11.1.1",
    "vitepress-markdown-timeline": "^1.2.1",
    "vitepress-plugin-announcement": "0.1.3",
    "vitepress-plugin-group-icons": "^1.2.4",
    "vitepress-plugin-mermaid": "2.0.13",
    "vitepress-plugin-pagefind": "0.4.11",
    "vitepress-plugin-rss": "0.3.0",
    "vitepress-plugin-tabs": "0.2.0",
    "cross-spawn": "^7.0.3",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "p-limit": "4",
    "@element-plus/icons-vue": "^2.3.1",
    "artalk": "^2.8.5",
    "element-plus": "^2.7.2",
    "pagefind": "^1.2.0",
    "sass": "^1.80.6",
    "typescript": "^5.4.5",
    "tsup": "^7.2.0",
    "vite": "^5.4.9",
    "vitepress": "1.5.0",
    "vitepress-plugin-51la": "0.1.0",
    "vue": "^3.5.12"
  },
  "directories": {
    "doc": "docs"
  },
  "author": "",
  "license": "MIT"
}
